{
    "manifest_version": 3,
    "name": "Block",
    "version": "1.0.0",
    "description": "Google Blokcer",
    "permissions":[
        "webNavigation",
        "tabs",
        "storage"
    ],
    "icons":{
      "64":"assets/block.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
   
}
